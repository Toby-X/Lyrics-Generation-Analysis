Theta.r = matrix(rep(0,I*K),nrow=I)
for (j in 1:K) {
Theta.r[,j] = runif(I,(j-1)/K,j/K)
}
c = rdirichlet(1,al)
c = cumsum(c)
for (i in 1:N) {
a = runif(1)
if (a<c[1]){
Pi.r[i,1] = 1
} else if (a<c[2]) {
Pi.r[i,2] = 1
} else if (a<c[3]) {
Pi.r[i,3] = 1
} else if (a<c[4]) {
Pi.r[i,4] = 1
} else {
Pi.r[i,5] = 1
}
}
